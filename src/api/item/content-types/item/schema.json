        "Favoritos",
        "Paquetes"
      ]
    },
    "longDescription": {
      "type": "richtext"
    },
    "descuento": {
      "type": "decimal"
    },
    "instock": {
      "type": "integer"
    },
    "unit": {
      "type": "enumeration",
      "enum": [
        "porci√≥n",
        "pieza",
        "grs.",
        "orden"
      ]
    },
    "restaurant": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::restaurant.restaurant",
      "inversedBy": "items"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "items"
    },
    "logo": {
      "allowedTypes": [
